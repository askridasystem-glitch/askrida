        </s:complexType>
      </s:element>
    </s:schema>
  </types>
  <message name="OtorisasiSoapIn">
    <part name="parameters" element="s0:Otorisasi" />
  </message>
  <message name="OtorisasiSoapOut">
    <part name="parameters" element="s0:OtorisasiResponse" />
  </message>
  <portType name="Service1Soap">
    <operation name="Otorisasi">
      <input message="s0:OtorisasiSoapIn" />
      <output message="s0:OtorisasiSoapOut" />
