	<queryString><![CDATA[SELECT * FROM 
T_ASN a 
inner join T_INVOICE i ON a.asn_id = i.asn_id
inner join V_VENDORS v ON i.vendor_id = v.vendor_id
inner join T_INVOICE_DETAILS id ON i.invoice_id =  id.invoice_id
inner join V_ITEM it ON id.item_id=it.item_id
where i.contract_number = $P{nomor_kontrak}
and v.vendor_id = $P{rekanan_pengadaan}
and i.ccy_code =$P{mata_uang}
and a.aw_number = $P{awb} 
and a.aw_ata=$P{tanggal_tiba}]]></queryString>
